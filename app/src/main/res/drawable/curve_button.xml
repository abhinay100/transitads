<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />

            <!--#5e7974-->
            <!--#345953  #689a92-->
            <stroke android:width="1dip" android:color="#ae4a07" />
            <!--<gradient android:angle="-90" android:startColor="#f7921e" android:endColor="#f7921e"  />-->
            <gradient android:angle="-90" android:startColor="#ae4a07" android:endColor="#ae4a07"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#f7921e" />
            <solid android:color="#f7921e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#f7921e" />
            <gradient android:angle="-90" android:startColor="#f7921e" android:endColor="#f7921e" />
        </shape>
    </item>
</selector>